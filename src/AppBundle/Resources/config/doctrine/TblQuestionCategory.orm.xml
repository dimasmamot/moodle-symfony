<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TblQuestionCategory" table="tbl_question_category">
    <id name="idQuestionCategory" type="integer" column="ID_QUESTION_CATEGORY">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="categoryName" type="string" column="CATEGORY_NAME" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
